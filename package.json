{
  "name": "resume-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/resources/scss --include-path ./node_modules ./src/resources/scss/style.scss -o ./src/resources/css",
    "watch-css": "npm run build-css && node-sass --include-path ./src/resources/scss --include-path ./node_modules ./src/resources/scss/style.scss -o ./src/resources/css --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5"
  }
}
